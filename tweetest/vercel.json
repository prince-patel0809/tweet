{
    "builds": [
        {
            "src": "tweetest/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLamdaSize": "15mb",
                "runtime": "python3.13.6"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "tweetest/wsgi.py"
        }
    ]
}